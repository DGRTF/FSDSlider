include ./../../blockUI/checkbox/checkbox

html
    head
        meta(charset="utf-8")
        meta(name="viewport" content="width=device-width, maximum-scale=1.0, minimum-scale=1.0")
        title="Cards"
    body
        .block
            .slider
                .panel-settings.js-panel-settings
                    .container-value
                        .span.name-value="Min possible value 12"
                        input.min-value.js-min-value-one(type="number" value="-100")
                    .container-value
                        .span.name-value="Max possible value"
                        input.max-value.js-max-value-one(type="number" value="100")
                    .container-value
                        .span.name-value="Min value"
                        input.min-select-value.js-min-select-value-one(type="number")
                    .container-value
                        .span.name-value="Max value"
                        input.max-select-value.js-max-select-value-one(type="number")
                    .container-value
                        .span.name-value="Step value"
                        input.step.js-step-one(type="number" value="1" min="0")
                    +checkbox({input: 'checkbox',contentHead: 'HiddenMin', mode: '_move'})
                    +checkbox({input: 'checkbo',contentHead: 'HiddenMax', mode: '_move'})
                .parent

